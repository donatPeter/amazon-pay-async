{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "lib": [
      "es2017",
      "esnext"
    ],
    "moduleResolution": "node",
    "noImplicitAny": true,
    "sourceMap": true,
    "outDir": ".build",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "rootDir": "./",
  },
  "exclude": [
    "node_modules/**",
  ]
}